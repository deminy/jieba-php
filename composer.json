{
    "name": "deminy/jieba-php",
    "type": "library",
    "description": "结巴中文分词PHP版本",
    "keywords": ["Jieba", "PHP", "NLP", "natural language processing", "Chinese"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "cache/cache": "~0.4",
        "league/csv": "~8.2",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "monolog/monolog": "~1.11",
        "mustache/mustache": "~2.11",
        "phpunit/phpunit": "~6.0",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Jieba\\": "src/Jieba/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "./bin/gen_dict_json.php"
        ],
        "post-update-cmd": [
            "./bin/gen_dict_json.php"
        ]
    },
    "suggest": {
        "ext-msgpack": "*"
    }
}
